<AnchorPane prefHeight="460.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.work.controller.segment.SegmentContext">
   <children>
      <ImageView fitHeight="460.0" fitWidth="700.0">
         <image>
            <Image url="@../../../images/download.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btnCansel" layoutX="559.0" layoutY="402.0" mnemonicParsing="false" onAction="#actionClose" prefHeight="43.0" prefWidth="122.0" style="-fx-background-color: white;" text="Назад">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Button>
      <TextField fx:id="textName" layoutX="151.0" layoutY="102.0" prefHeight="199.0" prefWidth="420.0">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
         <opaqueInsets>
            <Insets left="10.0" />
         </opaqueInsets>
      </TextField>
      <Text fill="WHITE" layoutX="199.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Направление и описание проекта">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </Text>
      <Button fx:id="btnSave" layoutX="283.0" layoutY="344.0" mnemonicParsing="false" onAction="#actionSave" prefHeight="51.0" prefWidth="155.0" style="-fx-background-color: white;" text="Создать">
